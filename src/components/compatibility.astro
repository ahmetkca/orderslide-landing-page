---
import ContentSection from "~/components/content-section.astro";
import CompatibilityList from "~/components/compatibility-list.astro";

const shops = [
  {
    title: "Etsy",
    icon: "shops/etsy",
    url: "https://www.etsy.com/",
  },
  {
    title: "Shopify",
    icon: "shops/shopify",
    url: "https://www.shopify.com/",
  },
  {
    title: "Amazon",
    icon: "shops/amazon",
    url: "https://www.amazon.com/",
  },
  {
    title: "Ebay",
    icon: "shops/ebay",
    url: "https://www.ebay.com/",
  },
  {
    title: "BigCommerce",
    icon: "shops/bigcommerce",
    url: "https://www.bigcommerce.com/",
  },
  // {
  //   title: "Lit",
  //   icon: "frameworks/lit",
  //   url: "https://lit.dev/",
  // },
];

const couriers = [
  {
    title: "UPS",
    icon: "couriers/ups",
    url: "https://www.ups.com/ca/en/Home.page?",
  },
  {
    title: "USPS",
    icon: "couriers/usps",
    url: "https://www.usps.com/",
  },
  {
    title: "FedEx",
    icon: "couriers/fedex",
    url: "https://www.fedex.com/en-us/home.html",
  },
  {
    title: "Canada Post",
    icon: "couriers/canadapost",
    url: "https://www.canadapost-postescanada.ca/cpc/en?name=tgt",
  },
  {
    title: "ShipStation",
    icon: "couriers/shipstation",
    url: "https://www.shipstation.com/ca/",
  },
 
];
---

<ContentSection title="Compatibility" id="compatibility">
  <Fragment slot="lead">
    Connect your stores with one-click to your favorite e-commerce platform.
  </Fragment>
  <CompatibilityList
    title="Shops"
    data={shops}
    url="https://docs.astro.build/core-concepts/framework-components/"
  />
  <CompatibilityList
    title="Couriers"
    data={couriers}
    url="https://docs.astro.build/guides/deploy/"
  />
</ContentSection>
